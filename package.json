    "helmet": "^3.23.2",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "node": "^14.4.0",
    "pg": "^8.2.1",
    "uuid": "^8.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
